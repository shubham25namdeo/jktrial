
import static groovy.json.JsonOutput.*
import groovy.transform.Field

public static final String PROD_MGMT_IP = "192.168.20.1"


node {
  datas = readYaml file: 'release.yml'
  def rootDir = pwd()
  def example = load "${rootDir}@script/Example.Groovy "
  example.exampleMethod()
  example.otherExampleMethod()
}
pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                echo "Got version as ${datas.version} "
                generate_data(PROD_MGMT_IP)
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}


def generate_data(prod) {
    print(prod)
    }
