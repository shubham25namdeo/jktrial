import static groovy.json.JsonOutput.*
import groovy.transform.Field

public static final String PROD_MGMT_IP = "192.168.20.1"



pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
           
                step(String GET_DATA = sh (
                    script: 'cat ${PWD}\\aa.mk',
                    returnStdout: true
                ).trim())
                echo "data : ${GET_DATA}"

                generate_data(PROD_MGMT_IP)
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}


def generate_data(prod) {
    print(prod)
    }
